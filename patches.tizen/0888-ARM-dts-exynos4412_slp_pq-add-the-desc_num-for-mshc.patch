 arch/arm/boot/dts/exynos4412-slp_pq.dts | 1 +
 1 file changed, 1 insertion(+)

diff --git a/arch/arm/boot/dts/exynos4412-slp_pq.dts b/arch/arm/boot/dts/exynos4412-slp_pq.dts
index 92bea1f..9f98e32 100644
